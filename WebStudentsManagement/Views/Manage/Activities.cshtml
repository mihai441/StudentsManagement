@model Activities
@{
    ViewData["Title"] = "Activities";
    ViewData.AddActivePage(ManageNavPages.Activities);
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Activity Name</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">1</th>
                    <td><a href="Activity">First Activity</a></td>
                </tr>
                <tr>
                    <th scope="row">2</th>
                    <td><a href="Activity">Second Activit</a></td>
                </tr>
                <tr>
                    <th scope="row">3</th>
                    <td><a href="Activity">Third Activit</a></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}